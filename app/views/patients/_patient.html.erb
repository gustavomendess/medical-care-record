<tr>
  <td><%= patient.name %></td>
  <td><%= patient.birth_date.strftime("%d/%m/%Y") %></td>
  <td><%= CPF.new(patient.cpf).formatted %></td>
  <td> <%= link_to "Alterar", edit_patient_path(patient), class:"btn btn-default"%> </td>
  <td><%= button_to "Remover", patient,
          method: :delete,
          class:"btn btn-danger",
          data: {confirm: "Tem certeza que quer remover o paciente?"} %></td>
</tr>
